<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Customer.
    -->
    <changeSet id="20201117215404-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="customer_id"
                                 baseTableName="customer_company"
                                 constraintName="fk_customer_company_customer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="customer"/>

        <addForeignKeyConstraint baseColumnNames="company_id"
                                 baseTableName="customer_company"
                                 constraintName="fk_customer_company_company_id"
                                 referencedColumnNames="id"
                                 referencedTableName="company"/>
        
        <addForeignKeyConstraint baseColumnNames="company_id"
                                 baseTableName="customer"
                                 constraintName="fk_customer_company_id"
                                 referencedColumnNames="id"
                                 referencedTableName="company"/>

    </changeSet>
</databaseChangeLog>
